{
  "plugins": ["@trivago/prettier-plugin-sort-imports", "prettier-plugin-tailwindcss"],
  "semi": false,
  "singleQuote": true,
  "printWidth": 100,
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^@/services/(.*)$",
    "^@/hooks/(.*)$",
    "^@/components/(.*)$",
    "^@/views/(.*)$",
    "^~/(.*)$",
    "^[./]"
  ],
  "tabWidth": 2,
  "bracketSpacing": true,
  "trailingComma": "es5",
  "bracketSameLine": true,
  "useTabs": false,
  "endOfLine": "lf",
  "importOrderSortSpecifiers": true,
  "importOrderCaseInsensitive": true
}
